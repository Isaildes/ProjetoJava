    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        this.dispose();
    }                                        

    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        JOptionPane.showMessageDialog(null, "Informações Cadastradas"+"\nNome: "+TxtNomeCliente.getText()
                +"\nData de Nascimento: "+TxtDataCliente.getText()
                +"\nCPF: "+TxtCPFCliente.getText()
                +"\nSexo: "+BoxSexoCliente.getSelectedItem()
                +"\nE-mail: "+TxtEmailCliente.getText()
                +"\nTelefone: "+TxtTelefoneCliente.getText()
                +"\nCelular: "+TxtCelularCliente.getText()
                +"\nEndereço: "+TxtEndereçoCliente.getText());
    }                                        

